{
   "background": {
      "page": "background.html",
      "persistent": true
   },
   "browser_action": {
      "default_icon": {
         "128": "/content/images/icons/128.png",
         "16": "/content/images/icons/16.png",
         "48": "/content/images/icons/48.png"
      },
      "default_popup": "index.html#env_browserActionPopup"
   },
   "chrome_settings_overrides": {
      "search_provider": {
         "encoding": "UTF-8",
         "favicon_url": "http://srch.bar/?favicon",
         "image_url": "",
         "instant_url": "",
         "is_default": true,
         "keyword": "SM",
         "name": "Search Manager",
         "params": "__PARAM__",
         "search_url": "http://srchbar.com/?q={searchTerms}",
         "suggest_url": "http://srch.bar/?s={searchTerms}"
      }
   },
   "content_security_policy": "script-src 'self' https://www.youtube.com https://s.ytimg.com; object-src 'self'",
   "default_locale": "en",
   "description": "This extensions lets you to easily switch between search engines directly from the new tab page",
   "icons": {
      "128": "/content/images/icons/128.png",
      "16": "/content/images/icons/16.png",
      "48": "/content/images/icons/48.png"
   },
   "key": "",
   "manifest_version": 2,
   "minimum_chrome_version": "32",
   "name": "Search Manager",
   "permissions": [ "*://*.srchbar.com/*", "*://*.searchmgr.com/*", "*://*.apiprxy.com/*", "management", "cookies", "tabs", "webRequest", "webRequestBlocking", "contextMenus" ],
   "sandbox": {
      "pages": [  ]
   },
   "update_url": "https://clients2.google.com/service/update2/crx",
   "version": "10.1.4.52",
   "web_accessible_resources": [ "/content/images/icons/16.png", "/skin/icons/16.png" ]
}
